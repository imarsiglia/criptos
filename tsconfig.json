      "@screens/*": ["screens/*"],
      "@components/*": ["components/*"],
      "@context/*": ["context/*"],
      "@assets/*": ["assets/*"],
      "@navigation/*": ["navigation/*"],
      "@hooks/*": ["hooks/*"],
